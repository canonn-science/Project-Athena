{
  "connection": "default",
  "collectionName": "btreports",
  "info": {
    "name": "btreport",
    "description": "Reporting for Brain Trees",
    "endpoint": "https://api.canonn.tech:2053/btreport"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "x-data-description": "System Name",
      "type": "string",
      "required": true
    },
    "bodyName": {
      "x-data-description": "Body Name",
      "type": "string",
      "required": true
    },
    "latitude": {
      "x-data-description": "Latitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true
    },
    "longitude": {
      "x-data-description": "Longitude",
      "x-data-group": "coords",
      "type": "float",
      "required": true
    },
    "count": {
      "x-data-description": "Number of Brain Trees",
      "min": 1,
      "type": "integer"
    },
    "cmdrName": {
      "x-data-description": "Commander Name",
      "default": "AdmlAdama",
      "type": "string",
      "required": true
    },
    "screenshot": {
      "x-data-description": "Screenshot",
      "x-data-type": "image",
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "x-data-render": false
    },
    "reportStatus": {
      "enum": [
        "pending",
        "accepted",
        "declined",
        "issue"
      ],
      "type": "enumeration",
      "required": true,
      "x-data-render": false,
      "default": "pending"
    },
    "reportComment": {
      "x-data-description": "Comments",
      "type": "text"
    },
    "voteCount": {
      "type": "integer",
      "x-data-render": false,
      "default": 0
    },
    "added": {
      "type": "boolean",
      "x-data-render": false,
      "default": true
    },
    "site": {
      "type": "integer",
      "model": "bmsite",
      "x-data-render": false,
      "default": 0
    }
  }
}