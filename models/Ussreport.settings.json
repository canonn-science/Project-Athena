{
  "connection": "default",
  "collectionName": "ussreports",
  "info": {
    "name": "ussreport",
    "description": "Automated journal reports for Unknown Signal Sources"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "systemName": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "cmdrName": {
      "type": "string"
    },
    "cmdrComment": {
      "type": "text"
    },
    "isBeta": {
      "type": "boolean"
    },
    "screenshot": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        " updated",
        " accepted",
        " declined",
        " issue"
      ]
    },
    "reportComment": {
      "type": "text"
    },
    "added": {
      "type": "boolean"
    },
    "site": {
      "model": "usssite"
    },
    "ussRawJson": {
      "type": "json"
    }
  }
}
